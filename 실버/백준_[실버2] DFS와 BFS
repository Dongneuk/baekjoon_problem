import sys
from collections import deque
input = sys.stdin.readline

n, m, v = map(int, input().split())
visited = [False] * (n+1)

graph = [[] for _ in range(n+1)]
for _ in range(m):
    a, b = map(int, input().split())
    graph[a].append(b)
    graph[b].append(a)

for i in range(len(graph)):
    graph[i].sort()


def dfs(start):
    print(start, end=' ')
    visited[start] = True
    for i in graph[start]:
        if visited[i] == False:
            dfs(i)
            visited[i] = True


def bfs(start):
    queue = deque([start])
    visited[start] = True

    while queue:
        now = queue.popleft()
        print(now, end=' ')
        visited[now] = True
        for i in graph[now]:
            if visited[i] == False:
                visited[i] = True
                queue.append(i)


dfs(v)
print()
visited = [False] * (n+1)
bfs(v)
